#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/eao/base/base.owl#hasInput
:hasInput rdf:type owl:ObjectProperty ;
          rdfs:domain :AnalysisStep ;
          rdfs:range :Data ;
          rdfs:comment "Defines a data entity that is used to provide information for one analysis step. The analysis step will make use of this information and produce zero or more data entities as outputs."@en ;
          rdfs:label "has input"@en .


###  http://purl.org/eao/base/base.owl#hasOutput
:hasOutput rdf:type owl:ObjectProperty ;
           rdfs:domain :AnalysisStep ;
           rdfs:range :Data ;
           rdfs:comment "Defines a data entity that was produced by an analysis step. This can be newly data generated by the analysis step, or derived data, based on one or more data inputs."@en ;
           rdfs:label "has output"@en .


###  http://purl.org/eao/base/base.owl#hasStep
:hasStep rdf:type owl:ObjectProperty ;
         rdfs:domain :CompoundAnalysis ;
         rdfs:range :AnalysisStep ;
         rdfs:comment "Defines an analysis step as part of a (larger) compound analysis."@en ;
         rdfs:label "has step"@en .


###  http://purl.org/eao/base/base.owl#isImplementedIn
:isImplementedIn rdf:type owl:ObjectProperty ;
                 rdfs:domain :AnalysisStep ;
                 rdfs:range :SoftwareImplementation .


###  http://purl.org/eao/base/base.owl#isImplementedInPackage
:isImplementedInPackage rdf:type owl:ObjectProperty ;
                        rdfs:domain :SoftwareImplementation ;
                        rdfs:range :SoftwarePackage .


###  http://purl.org/eao/base/base.owl#usesParameter
:usesParameter rdf:type owl:ObjectProperty ;
               rdfs:domain :AnalysisStep ;
               rdfs:range :AnalysisParameter .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/eao/base/base.owl#nameInDefinition
:nameInDefinition rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Function .


###  http://purl.org/eao/base/base.owl#packageVersion
:packageVersion rdf:type owl:DatatypeProperty ;
                rdfs:domain :SoftwarePackage ;
                rdfs:range rdfs:Literal .


###  http://purl.org/eao/base/base.owl#programName
:programName rdf:type owl:DatatypeProperty ;
             rdfs:domain :Program ;
             rdfs:range rdfs:Literal .


###  http://purl.org/eao/base/base.owl#version
:version rdf:type owl:DatatypeProperty ;
         rdfs:domain :SoftwareImplementation ;
         rdfs:range rdfs:Literal .


#################################################################
#    Classes
#################################################################

###  http://purl.org/eao/base/base.owl#AnalysisElement
:AnalysisElement rdf:type owl:Class ;
                 rdfs:comment "Describes the fundamental elements of one analysis."@en ;
                 rdfs:label "analysis element"@en .


###  http://purl.org/eao/base/base.owl#AnalysisParameter
:AnalysisParameter rdf:type owl:Class ;
                   rdfs:subClassOf :AnalysisElement ;
                   rdfs:comment "An information entity that controls the behavior of an analysis step. It does not provide data, but rather changes how the computations operate when transforming or generating new data."@en ;
                   rdfs:label "parameter"@en .


###  http://purl.org/eao/base/base.owl#AnalysisStep
:AnalysisStep rdf:type owl:Class ;
              rdfs:subClassOf :AnalysisElement ;
              rdfs:comment "A process that generates new data entities or performs specific operations (clean, select, transform, or inspect/visualize) using existing data entities. Ultimately, the goal is to obtain useful information from the data. An analysis step can take zero or more data inputs and can generate one or more data outputs. The behavior of the operation can be controlled by zero or more parameters. A complete analysis will be composed by one or more analysis steps."@en ;
              rdfs:label "analysis step"@en .


###  http://purl.org/eao/base/base.owl#CompoundAnalysis
:CompoundAnalysis rdf:type owl:Class ;
                  rdfs:comment "An analysis process that is composed by one or more analysis steps. This represents the sequential nature of some analyses, composed by several small steps that require data produced by another analysis steps. However, the data input is passed to the first step in the sequence, and the final data output of the analysis is the output of the last step in the sequence."@en ;
                  rdfs:label "compound analysis"@en .


###  http://purl.org/eao/base/base.owl#Data
:Data rdf:type owl:Class ;
      rdfs:subClassOf :AnalysisElement ;
      rdfs:comment "An entity that contains relevant information during the analysis. It can be recorded from a biological entity or generated/transformed by analysis step processes."@en ;
      rdfs:label "data"@en .


###  http://purl.org/eao/base/base.owl#DataRepresentation
:DataRepresentation rdf:type owl:Class ;
                    rdfs:comment """Defines the structure to represent the data. 

For example, voltage potentials recorded using extracellular electrodes are usually continuously acquired using a regular sampling interval, constituting a time series."""@en ;
                    rdfs:label "data representation"@en .


###  http://purl.org/eao/base/base.owl#ElectrophysiologySignalSource
:ElectrophysiologySignalSource rdf:type owl:Class ;
                               rdfs:comment """Defines the source of the recorded electrophysiology signal. This is a general concept, that can be expanded to incorporate the anatomica/tissue description from where the signal was recorded, as well as the recording technique.

For example, voltage potentials can be acquired from extracellular electrodes situtated on the scalp (electroencephalogram), directly on the cortical surface (electrocorticogram) or implanted deep into a specific brain region (multielectrode array)."""@en ;
                               rdfs:label "electrophysiology signal source"@en .


###  http://purl.org/eao/base/base.owl#Function
:Function rdf:type owl:Class ;
          rdfs:subClassOf :SoftwareImplementation ;
          rdfs:comment "A reusable set of instructions that can be used when writing computer programs. A function performs specific tasks, and can be implemented as part of a single computer program or in a software package (library) so that it can be reused across several programs."@en ;
          rdfs:label "function"@en .


###  http://purl.org/eao/base/base.owl#Program
:Program rdf:type owl:Class ;
         rdfs:subClassOf :SoftwareImplementation ;
         rdfs:comment "A set of instructions that is executed by a computer. The instructions of the program can be executed by an interpreter or compiled to machine code. The program instructs the computer to perform tasks and it is called by the operating system."@en ;
         rdfs:label "program"@en .


###  http://purl.org/eao/base/base.owl#SoftwareImplementation
:SoftwareImplementation rdf:type owl:Class ;
                        owl:disjointWith :SoftwarePackage ;
                        rdfs:comment "Describes the details for the execution of the analysis step using a computer. The software contains a sequence or set of instructions that is used by the computer to perform all the computations required to execute the analysis step."@en ;
                        rdfs:label "software implementation"@en .


###  http://purl.org/eao/base/base.owl#SoftwarePackage
:SoftwarePackage rdf:type owl:Class ;
                 rdfs:comment "A collection of one or more programs that can be executed by a computer and/or one or more functions that can be imported and used when writing programs."@en ;
                 rdfs:label "software package"@en .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :AnalysisParameter
                :AnalysisStep
                :Data
              )
] .


